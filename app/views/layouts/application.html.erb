<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  

</head>
<body>
   <!DOCTYPE google>
     <!DOCTYPE google>
      <title>Google Auth Example App</title>
    <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
    <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
    <%= csrf_meta_tags %>
    
    <!DOCTYPE google>
  <div>
      <% if current_user %>
        Signed in as <strong><%= current_user.name %></strong>!
        <%= link_to "Sign out", signout_path, id: "sign_out" %>
      <% else %>
        <%= link_to "Sign in with Google", "/auth/google_oauth2", id: "sign_in" %>
      <% end %>
    </div>
    <div>
 
    </div>
    
     <!DOCTYPE google>
  <%= nav_bar brand:"ESTOQUE", brand_link: root_url do %>
    <%= form_tag busca_produto_path, method:
                  :get, 
                  class: "navbar-form navbar-right" do%>
      <div class="form-group">
        <%= text_field_tag :nome, @nome_a_buscar, class: "form-control"%>
        <%= button_to "Buscar", nil, class:"btn btn-default" %>
      </div>
    <% end %>
  <% end %>
<div class="container">
  <%= yield %>
</div>
</body>
</html>
